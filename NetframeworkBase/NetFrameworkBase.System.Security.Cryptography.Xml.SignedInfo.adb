   begin
      return RetVal : NetFrameworkBase.System.Security.Cryptography.Xml.SignedInfo.Kind_Ptr := new NetFrameworkBase.System.Security.Cryptography.Xml.SignedInfo.Kind do
          NetFrameworkAdaRuntime.CreateInstance (RetVal.m_Kind, This_AssemblyName, This_TypeName, Instance, NetFrameworkWin32.BindingFlags'(CreateInstance)'Enum_rep, null);
      end return;
   end;
   
end;
