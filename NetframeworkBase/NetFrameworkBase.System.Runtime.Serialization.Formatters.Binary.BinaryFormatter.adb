   procedure Serialize
   (
      this : in out BinaryFormatter.Kind;
      serializationStream : NetFrameworkBase.System.IO.Stream.Kind_Ptr; 
      graph : NetFrameworkBase.System.Object.Kind_Ptr; 
      headers : NetFrameworkBase.System.Runtime.Remoting.Messaging.Header.Kind_Array
   ) is
